{
  "name": "pick-formatter",
  "displayName": "Pick Formatter",
  "description": "Pick Basic formatter",
  "version": "0.7.4",
  "publisher": "scragg0x",
  "repository": "https://github.com/DSI-Solutions/pick-formatter.git",
  "engines": {
    "vscode": "^1.14.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:pick"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "languages": [
      {
        "id": "pick",
        "aliases": [
          "pick"
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "typescript": "^3.8.3",
    "vscode": "^1.1.36",
    "mocha": "^7.1.1",
    "@types/node": "^13.11.1",
    "@types/mocha": "^7.0.2"
  }
}

{
    "name": "pick-formatter",
    "displayName": "Pick Formatter",
    "description": "Pick Basic formatter",
    "version": "0.3.0",
    "publisher": "scragg0x",
    "engines": {
        "vscode": "^1.14.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:pick"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "languages": [
            {
                "id": "pick",
                "aliases": [
                    "pick"
                ]
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test",
        "publish": "vsce publish"
    },
    "devDependencies": {
        "typescript": "^2.9.2",
        "vscode": "^1.1.18",
        "mocha": "^5.2.0",
        "@types/node": "^10.5.4",
        "@types/mocha": "^5.2.5"
    }
}
